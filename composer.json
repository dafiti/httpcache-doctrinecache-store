{
    "name": "httpcache/doctrinecache-store",
    "description": "Doctrine Cache Store for Symfony Http Cache",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "symfony",
        "http",
        "cache",
        "doctrine"
    ],
    "authors": [
        {
            "name": "Fabio Ribeiro",
            "email": "fabiorphp@gmail.com"
        }
    ],
    "support": {
        "issue": "https://github.com/fabiorphp/httpcache-doctrinecache-store/issues"
    },
    "minimum-stability": "dev",
    "require-dev": {
        "fabpot/php-cs-fixer": "@stable",
        "lstrojny/phpunit-function-mocker": "0.2.0",
        "phpunit/phpunit": "@stable",
        "doctrine/cache": "~1.4.1",
        "symfony/http-kernel": "~2.7.0"
    },
    "autoload": {
        "psr-4": {
            "Dafiti\\HttpCache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dafiti\\HttpCache\\": "tests/"
        }
    }
}
